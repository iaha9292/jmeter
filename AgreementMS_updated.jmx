              <stringProp name="script">// File path (modify the path to your actual CSV file location)
String filePath = &quot;/Users/gayanherath/Documents/apache-jmeter-5.6.3/bin/examples/agreementid.csv&quot;;

// Create a BufferedReader to read the file
BufferedReader br = new BufferedReader(new FileReader(filePath));

// Read current line number from a JMeter variable (this will track the line being read)
int currentLine = Integer.parseInt(vars.get(&quot;currentLine&quot;) == null ? &quot;0&quot; : vars.get(&quot;currentLine&quot;));

// Skip lines until the current line (if necessary)
String line = null;
int count = 0;
while ((line = br.readLine()) != null) {
    if (count == currentLine) {
        break;
    }
    count++;
}

// Check if the file has been completely read
if (line != null) {

    // Assign the values to JMeter variables
    vars.put(&quot;id&quot;, line);
  

    // Update the current line number for the next iteration
    vars.put(&quot;currentLine&quot;, String.valueOf(currentLine + 1));
} else {
    // If the end of the file is reached, reset the current line (or stop the test)
    vars.put(&quot;currentLine&quot;, &quot;0&quot;);  // Reset to the beginning
    // You can also stop the thread if needed
    // SampleResult.setStopThread(true);
}

// Close the file reader
br.close();
</stringProp>
